<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>profile_page</title>
    <link rel="stylesheet" href="/static/profile.css" />
  </head>
  <body>
    <%- include('./component/createNav.ejs') %>
    <h1>Hello welcome to our site</h1>
    <h2 class="fri"> Friends</h2>
    <% user.forEach(users => { %>
        <ul>
          <li><%= users.name %></li>
        </ul>
      <% }) %>
      <h2 class="fri">Posts by friends</h2>
        <% posts.forEach(post => { %>
            <h4><%= post.title %></h4>
            <ul>
            <li><%= post.body %></li>
          </ul>
        <% }) %>
        <%-include("./component/footer.ejs") %>
  </body>
</html>
